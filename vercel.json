{
  "version": 2,
  "builds": [
    {
      "src": "api/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/(.*)"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/contact",
      "dest": "/api/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
